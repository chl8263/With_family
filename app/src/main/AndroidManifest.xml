<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.with_family">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.READ_SMS" />
    <uses-permission android:name="android.permission.READ_PHONE_NUMBERS" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme2" />
        <activity
            android:name=".RegisterActivity"
            android:theme="@android:style/Theme.Dialog" />
        <activity
            android:name=".ui.f_calendar.Calendar_dialog"
            android:theme="@style/Theme.AppCompat.Dialog" />
        <activity android:name=".ui.f_home.UserInfoActivity" />
        <activity android:name=".ui.f_home.UserInfoActivity2" />
        <activity
            android:name=".ui.f_home.AddFriendActivity"
            android:theme="@style/Theme.AppCompat.Dialog" />
        <activity
            android:name=".ui.f_home.CreateRoomActivity"
            android:theme="@style/Theme.AppCompat.Dialog"/>
    </application>

</manifest>